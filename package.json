{
  "name": "optichannel",
  "version": "0.1.0",
  "description": "Gasless options trading protocol built on Yellow Network state channels",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "server": "tsx src/server.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:connection": "tsx src/test-connection.ts",
    "test:options": "tsx src/test-options.ts",
    "test:trading": "tsx src/test-trading.ts",
    "test:marketplace": "tsx src/test-marketplace.ts",
    "test:portfolio": "tsx src/test-portfolio.ts",
    "test:strategies": "tsx src/test-strategies.ts",
    "test:api": "tsx src/test-api.ts",
    "test:db": "tsx src/test-db.ts",
    "test:flow": "tsx src/test-full-flow.ts",
    "test:sim": "tsx src/test-simulation.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "options",
    "defi",
    "yellow-network",
    "state-channels",
    "ethereum"
  ],
  "author": "Aayush",
  "license": "MIT",
  "dependencies": {
    "@circle-fin/developer-controlled-wallets": "^10.1.0",
    "@erc7824/nitrolite": "^0.5.3",
    "@pythnetwork/hermes-client": "^1.4.0",
    "@supabase/supabase-js": "^2.93.3",
    "cors": "^2.8.6",
    "dotenv": "^16.4.0",
    "express": "^5.2.1",
    "viem": "^2.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
